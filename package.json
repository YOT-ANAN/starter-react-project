{
  "name": "starter-react-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "postcss-cli": "^8.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "tailwindcss": "^1.9.6",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start:tw": "postcss ./src/styles/index.css -o ./src/styles/tailwind.css -w",
    "start:react": "react-scripts start",
    "start": "concurrently \"npm run start:tw\" \"npm run start:react\"",
    "build:tw": "postcss ./src/styles/index.css -o ./src/styles/tailwind.css --env production",
    "build": "npm run build:tw && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
